RewriteEngine On
RewriteBase /poopiyohe/

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^admin$ admin.php
RewriteRule ^index$ index.php
RewriteRule ^daftar daftar.php

RewriteRule ^beranda$ admin.php?views_admin=beranda
RewriteRule ^kemlem$ admin.php?views_admin=kemlem
RewriteRule ^instansi_vertikal$ admin.php?views_admin=instansi_vertikal
RewriteRule ^opd$ admin.php?views_admin=opd
RewriteRule ^auditor$ admin.php?views_admin=auditor
RewriteRule ^auditan$ admin.php?views_admin=auditan



RewriteRule ^([a-zA-Z0-9_-]+)$ admin.php?views_admin=$1


Options All -Indexes